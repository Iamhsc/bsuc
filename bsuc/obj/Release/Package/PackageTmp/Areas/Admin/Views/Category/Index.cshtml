@model IEnumerable<bsuc.common.Model.Bsuc_Protal_Category>
@{
    bsuc.common.Model.BsucConnectext db=new bsuc.common.Model.BsucConnectext();
}
<div class="layui-form">
    <table class="layui-table mt10" lay-even="" lay-skin="row">
        <colgroup>
            <col width="50">
            <col>
            <col>
            <col width="90">
            <col width="200">
        </colgroup>
        <thead>
            <tr>
                <th>
                    <input type="checkbox" lay-skin="primary" lay-filter="allChoose"></th>
                <th>类别编号</th>
                <th>类别名称</th>
                <th>类别描述</th>
                <th>添加时间</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                var ctime=bsuc.common.Common.IntToDateTime(item.ctime,"yyyy-MM-dd HH:mm:ss");
                <tr>
                    <td>
                        <input type="checkbox" class="layui-checkbox checkbox-ids" name="ids[]" lay-skin="primary"></td>
                    <td>@Html.DisplayFor(menu => item.id)</td>
                    <td>@Html.DisplayFor(menu => item.catname)</td>
                    <td>@Html.DisplayFor(menu => item.description)</td>
                    <td>@ctime</td>
                    <td>
                        <div class="layui-btn-group">
                            <div class="layui-btn-group">
                                <a href="/admin/category/edit?id=@Html.DisplayFor(itemmodel=>item.id)" class="layui-btn layui-btn-primary layui-btn-sm"><i class="layui-icon">编辑</i></a>
                                <a data-href="/admin/category/Delete?id=@Html.DisplayFor(itemmodel=>item.id)" class="layui-btn layui-btn-primary layui-btn-sm j-tr-del"><i class="layui-icon">删除</i></a>
                            </div>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
